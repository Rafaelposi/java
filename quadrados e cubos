public class TabelaQuadradosCubos {
    public static void main(String[] args) {
        System.out.println("NÃºmero   Quadrado   Cubo");
        for (int i = 0; i <= 10; i++) {
            int quadrado = i * i;
            int cubo = i * i * i;
            System.out.printf("%-8d %-10d %d%n", i, quadrado, cubo);
        }
    }
}
